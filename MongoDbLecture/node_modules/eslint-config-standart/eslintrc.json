{
  "extends": "standard",

  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module",
    "impliedStrict": true
  },
  "rules": {
    "comma-style": 0,
    "operator-linebreak": 0,
    "space-unary-ops": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "indent": 0,
    "arrow-parens": 0
  }
}
